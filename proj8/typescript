 * Date: 4.5.16
 */

 #include "ArrayQueueTester.h"

#include "PalindromeDetectorTester.h"
 
 int main() {
 	ArrayQueueTester aqt;
 	aqt.runTests();
	PalindromeDetectorTester pt;
	pt.runTests();
 }
 
 
